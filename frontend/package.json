{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Nuxt.js project with ServiceStack backend",
  "author": "Matt &lt;@mattjcowan&gt;",
  "private": true,
  "scripts": {
    "clean": "rimraf ./.nuxt & rimraf ./dist",
    "server": "cd ../backend/WebApp && dotnet restore && dotnet build && dotnet run",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "npm run clean && nuxt generate && cpx \"dist/**/*.*\"  ../backend/WebApp/wwwroot",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/proxy": "1.3.1",
    "cpx": "1.5.0",
    "cross-env": "^5.2.0",
    "dotenv": "6.2.0",
    "js-cookie": "2.2.0",
    "jwt-decode": "2.2.0",
    "normalize.css": "8.0.1",
    "nuxt": "^2.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "4.11.0",
    "nodemon": "^1.11.0",
    "sass-loader": "7.1.0"
  }
}
